<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" path="webview.db"/>
        <exclude domain="database" path="webviewCache.db"/>
        <exclude domain="file" path="webview"/>
        <exclude domain="file" path="cache"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="database" path="webview.db"/>
        <exclude domain="database" path="webviewCache.db"/>
        <exclude domain="file" path="webview"/>
        <exclude domain="file" path="cache"/>
    </device-transfer>
</data-extraction-rules> 